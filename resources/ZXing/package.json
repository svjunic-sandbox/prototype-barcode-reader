    "@babel/runtime": "^7.0.0",
    "babel-loader": "^8.0.6",
    "browserslist": "^4.3.5",
    "core-js": "^3.1.4",
    "regenerator-runtime": "^0.13.2",
    "regenerator-transform": "^0.13.3",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5"
  },
  "dependencies": {
    "@zxing/library": "^0.15.2"
  }
